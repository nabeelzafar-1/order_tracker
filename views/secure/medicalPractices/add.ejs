<%- include('../../includes/header') -%>
  <div id="main">
    <div
      class="d-flex justify-content-between flex-wrap flex-md nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
      <h1 class="h2">Medical Practice</h1>
      <div class="btn-toolbar mb-2 mb-md 0">
        <div class="btn-group me-2">
          <button class="btn btn-sm btn-outline-secondary">?</button>
        </div>
      </div>
    </div>

    <div class="col py-3">
      <div class="row">
        <div class="col">
          <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
              <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
              <li class="breadcrumb-item active">New Medical Practice</li>
            </ol>
          </nav>
        </div>
        <div class="col text-end fw-lighter">

        </div>
      </div>

    </div>


    <form action="/api/secure/medicalPractice/add" method="POST" name="f1" onsubmit="return validate()">
      <div class="row form-group mb-4">
        <div class="col">
          <label for="practiceName">Practice Name</label>
          <input type="text" class="form-control" id="practiceName" name="practiceName" value=""
            placeholder="Practice Name" required>
          <span id="practicenamelocation" style="color:red"></span></td>
          </tr>
        </div>

        <div class="col">
          <label for="phone">Phone</label>
          <input type="number" class="form-control" id="phone" name="phone" value="" placeholder="Phone" required>
        </div>
      </div>
      <div class="row form-group mb-4">
        <div class="col">
          <label for="mailingAddress">Mailing Address</label>
          <input type="text" class="form-control" id="mailingAddress" name="mailingAddress" value=""
            placeholder="Mailing Address" required>
        </div>

        <div class="col">
          <label for="mailingCity">Mailing City</label>
          <input type="text" class="form-control" id="mailingCity" name="mailingCity" value=""
            placeholder="Shipping City" required>
        </div>
        <div class="col">
          <label for="mailingState">Mailing State</label>
          <input type="text" class="form-control" id="mailingState" name="mailingState" value=""
            placeholder="Mailing Sate" required>
        </div>

        <div class="col">
          <label for="mailingZip">Mailing Zip</label>
          <input type="number" class="form-control" id="mailingZip" name="mailingZip" value="" placeholder="Mailing Zip"
            required>
        </div>

      </div>
      <div class="row form-group mb-4">
        <div class="col">
          <label for="shippingAddress">Shipping Address</label>
          <input type="text" class="form-control" id="shippingAddress" name="shippingAddress" value=""
            placeholder="Shipping Address" required>
        </div>

        <div class="col">
          <label for="shippingCity">Shipping City</label>
          <input type="text" class="form-control" id="shippingCity" name="shippingCity" value=""
            placeholder="Shipping Address" required>
        </div>
        <div class="col">
          <label for="shippingState">Shippinge State</label>
          <input type="text" class="form-control" id="shippingeState" name="shippingState" value=""
            placeholder="Shipping Address" required>
        </div>

        <div class="col">
          <label for="shippingZip">Shipping Zip</label>
          <input type="number" class="form-control" id="shippingZip" name="shippingZip" value=""
            placeholder="Shipping Zip" required>
        </div>

      </div>
      <div class="row form-group mb-4">
        <div class="col">
          <label for="fax">Fax</label>
          <input type="number" class="form-control" id="fax" name="fax" value="" placeholder="Fax" required>
        </div>

        <div class="col">
          <label for="website">Website</label>
          <input type="text" class="form-control" id="website" name="website" value="" placeholder="Website" required>
        </div>
      </div>
      <div class="row form-group mb-4">
        <div class="form-group">
          <label for="status">Status</label>
          <select class="form-select" id="status" name="status"  value="" >
            <option id="status" name="status"  value="Active">Active</option>
            <option id="status" name="status"  value="Closed">Closed</option>
            <option id="status" name="status"  value="Removed">Removed</option>
          </select>
        </div>
      </div>
      <div class="form-group mb-4">
        <button type="submit" class="btn btn-primary" value="register">Add </button>
      </div>

    </form>
    <div class="table-responsive">

      <div class="table-responsive">

        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th scope="col">Practice Name</th>
              <th scope="col">Phone</th>
              <th scope="col">Mailing Address</th>
              <th scope="col">Mailing City</th>
              <th scope="col">Mailing State</th>
              <th scope="col">Mailing Zip</th>
              <th scope="col">Shipping Address</th>
              <th scope="col">Mailing City</th>
              <th scope="col">Shipping State</th>
              <th scope="col">Shipping Zip</th>
              <th scope="col">Fax</th>
              <th scope="col">Website</th>
              <th scope="col" class="text-end">Status</th>
            </tr>
          </thead>


          <tbody>
            <% medicalPractice.forEach(element=> { %>
              <tr class="align-middle">
                <td>
                  <%= element.practiceName %>
                </td>
                <td>
                  <%= element.phone %>
                </td>
                <td>
                  <%= element.mailingAddress %>
                </td>
                <td>
                  <%= element.mailingCity %>
                </td>
                <td>
                  <%= element.mailingState %>
                </td>
                <td>
                  <%= element.mailingZip %>
                </td>
                <td>
                  <%= element.shippingAddress %>
                </td>
                <td>
                  <%= element.shippingCity %>
                </td>
                <td>
                  <%= element.shippingState %>
                </td>
                <td>
                  <%= element.shippingZip %>
                </td>
                <td>
                  <%= element.fax %>
                </td>
                <td>
                  <%= element.website %>
                </td>
                <td>
                  <%= element.status %>
                </td>
                <td class="text-end">

                  <div class="d-flex flex-row justify-content-end gap-2">

                    <a href="/view/<%= element._id %>" type="button" class="btn btn-primary btn-small">
                      <i class="bi bi-eye"></i>
                    </a>

                    <a href="/medicalprofessional/add/<%= element._id %>" type="button"
                      class="btn btn-primary btn-small a">
                      <small>Professional</small>
                    </a>

                    <a href="/edit/<%= element._id %>" type="button" class="btn btn-warning btn-small">
                      <i class="bi bi-pencil"></i>
                    </a>

                    <a href="/api/secure/medicalPractice/delete/<%= element._id %>"
                      class="btn btn-danger btn-small">
                      <i class="bi bi-person-x"></i>
                    </a>

                  </div>


                </td>
              </tr>
              <% }) %>
          </tbody>
        </table>
      </div>
    </div>
  </div>

  <%- include('../../includes/footer') -%>