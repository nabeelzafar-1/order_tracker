<%- include('../../includes/header') -%>
<div id="main">
<div class="d-flex justify-content-between flex-wrap flex-md nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
  <h1 class="h2">Customer</h1>
  <div class="btn-toolbar mb-2 mb-md 0">
    <div class="btn-group me-2">
      <button class="btn btn-sm btn-outline-secondary">?</button>
    </div>
  </div>
</div>

<div class="col py-3">
  <div class="row">
    <div class="col">
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
          <li class="breadcrumb-item active">New Customer</li>
        </ol>
      </nav>
    </div>
    <div class="col text-end fw-lighter">
      <b>UserId</b>
    </div>
  </div>

</div>


<form action="/api/secure/medicalPractice/add" method="POST"  name="f1"  onsubmit="return validate()">

  <div class="row form-group mb-4">
    <div class="col">
      <label for="practiceName">Practice Name</label>
      <input type="text"  class="form-control" id="practiceName" name="practiceName" value="" placeholder="Practice Name" required>
      <span id="practicenamelocation" style="color:red"></span></td></tr> 
    </div>

    <div class="col">
      <label for="phone">Phone</label>
      <input type="text" class="form-control" id="phone" name="phone" value="" placeholder="Phone" required>
    </div>
  </div>
  <div class="row form-group mb-4">
    <div class="col">
      <label for="fex">fex</label>
      <input type="text" class="form-control" id="fax" name="fax" value="" placeholder="fax" required>
    </div>

    <div class="col">
      <label for="website">website</label>
      <input type="text" class="form-control" id="website" name="website" value="" placeholder="website" required>
    </div>
  </div>

  <div class="row form-group mb-4">
    <div class="col">
      <label for="maillingAddress">Mail Addresse</label>
      <input type="text" class="form-control" id="mailingAddress" name="mailingAddress" value="" placeholder="Mail Addresse" required>
    </div>

    <div class="col">
      <label for="mailingCity">Mailing City</label>
      <input type="text" class="form-control" id="mailingCity" name="mailingCity" value="" placeholder="Shipping Address" required>
    </div>
    <div class="col">
      <label for="mailingeState">Mailinge State</label>
      <input type="text" class="form-control" id="mailingeState" name="mailingeState" value="" placeholder="Shipping Address" required>
    </div>

    <div class="col">
      <label for="mailingZip">Mailing Zip</label>
      <input type="text" class="form-control" id="mailingZip" name="mailingZip" value="" placeholder="mailingZip" required>
    </div>
 
   
  </div>
  <div class="row form-group mb-4">
    <div class="col">
      <label for="shippingAddress">shipping Addresse</label>
      <input type="text" class="form-control" id="shippingAddress" name="shippingAddress" value="" placeholder="Mail Addresse" required>
    </div>

    <div class="col">
      <label for="shippingCity">shipping City</label>
      <input type="text" class="form-control" id="shippingCity" name="shippingCity" value="" placeholder="Shipping Address" required>
    </div>
    <div class="col">
      <label for="shippingeState">shippinge State</label>
      <input type="text" class="form-control" id="shippingeState" name="shippingeState" value="" placeholder="Shipping Address" required>
    </div>

    <div class="col">
      <label for="shippingZip">shipping Zip</label>
      <input type="text" class="form-control" id="shippingZip" name="shippingZip" value="" placeholder="shippingZip" required>
    </div>
  </div>
  <div class="row form-group mb-4">
    <div class="col">
      <label for="status">status</label>
      <input type="text" class="form-control" id="status" name="status" value="" placeholder="status" required>
    </div>

  </div>
  <div class="form-group mb-4">
    <button type="submit" class="btn btn-primary" value="register">Add Customer</button>
  </div>

  

</form>
<div class="table-responsive">

  <div class="table-responsive">

    <table class="table table-striped table-sm">  
      <thead>
        <tr>
          <th scope="col">Practice Name</th>
          <th scope="col">Phone</th>
          <th scope="col">Mailing Address</th>
          <th scope="col">Shipping Address</th>
          <th scope="col">Fax</th>
          <th scope="col">Website</th>
          <th scope="col" class="text-end">Status</th>
        </tr>
      </thead>
  

      <tbody>
        <% medicalPractices.forEach(element => { %>
          <tr class="align-middle">
            <td><%= element.practiceName %></td>
            <td><%= element.phone %></td>
            <td><%= element.maillingAddress %></td>
            <td><%= element.shippingAddress %></td> 
            <td><%= element.fax %></td>
            <td><%= element.website %></td>
            <td><%= element.status %></td>
            <td class="text-end">
  
              <div class="d-flex flex-row justify-content-end gap-2">
  
                <a href="/medicalprofessional/add/<%= element._id %>" type="button" class="btn btn-primary btn-small">
                  <i class="bi bi-eye"></i>
                </a>
  
                <a href="/edit/<%= element._id %>" type="button" class="btn btn-warning btn-small">
                  <i class="bi bi-pencil"></i>
                </a>
  
                <form action="/edit/<%= element._id %>?_method=DELETE" method="POST" class="position-relative">
                  <button type="submit" class="btn btn-danger btn-small">
                    <i class="bi bi-person-x"></i>
                  </button>
                </form>
  
              </div>
  
  
            </td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</div>
</div>

<%- include('../../includes/footer') -%>